section.animacion
	animation 0.5ms animanimacion infinite
@keyframes animanimacion
	from
		transform translatex(0px)
	to
		transform translatex(20px)
div.forma
	height 200px
	width 200px
	background-color aqua
	border-radius 50%
	margin 200px auto
	animation 0.5s animforma infinite ease-in-out alternate
@keyframes animforma
	from
		transform translatey(0px)
	to 
		transform translatey(-160px)